<Window x:Class="ImageLad.Views.Views.ChartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:ImageLad.ViewModels;assembly=ImageLad.ViewModels"
        xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:converters="clr-namespace:ImageLad.Views.Converters"
        mc:Ignorable="d"
        Title="ChartWindow" Height="400" Width="400"
        d:DataContext="{d:DesignInstance {x:Type vm:ChartViewModel}}">
    <Grid>
        <Grid.Resources>
            <converters:DoubleArray2Model x:Key="Array2Series" />
        </Grid.Resources>
        <WpfPlot x:Name="_Plot_">
        </WpfPlot>
    </Grid>
</Window>